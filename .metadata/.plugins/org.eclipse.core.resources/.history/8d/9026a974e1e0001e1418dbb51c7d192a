package day01;

import java.util.Scanner;

public class Ex14 {
	public static void main(String[] args) {
//		14. 사용자로부터 세 개의 정수형태의 
//		문자열을 입력받아 문자열, 정수, 실수를 출력하기(.next만 사용)
		
		String txt = "";
		String num = "";
		
		Scanner sc= new Scanner(System.in);
		
		txt = sc.next();
		System.out.println(txt);
		num = sc.next();
		System.out.println(num);
	}
}
